{
    "name": "@thinairthings/uix",
    "author": "Dan Lannan",
    "version": "2.4.0",
    "type": "module",
    "types": "./dist/lib/index.d.ts",
    "bin": {
        "uix": "./dist/cli/cli.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "nodemon",
        "build": "tsup-node",
        "build:run": "tsup-node && node dist/cli/cli.js",
        "test": "dotenvx run -f .env.test -- pnpm build && dotenvx run -f .env.test -- vitest run basic --test-timeout=100000",
        "test:match": "pnpm build && dotenvx run -f .env.test -- vitest run match --test-timeout=100000",
        "uix": "dotenvx run -f .env.test -- node ./dist/cli/cli.js --config=./tests/uix/uix.config.ts",
        "wipset": "changeset && changeset version && changeset publish"
    },
    "exports": {
        ".": {
            "types": "./dist/lib/index.d.ts",
            "import": "./dist/lib/index.js",
            "require": "./dist/lib/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ThinAirThings/uix.git"
    },
    "keywords": [],
    "devDependencies": {
        "@babel/preset-env": "^7.24.7",
        "@changesets/cli": "^2.27.6",
        "@sindresorhus/tsconfig": "^5.0.0",
        "@types/node": "^20.14.9",
        "@types/react": "^18.3.3",
        "@types/uuid": "^10.0.0",
        "nodemon": "^3.1.3",
        "tsup": "^8.1.0",
        "typescript": "^5.4.5",
        "vite": "^5.3.1",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.32.0",
        "@thinairthings/utilities": "^0.2.2",
        "bundle-n-require": "^1.1.1",
        "dedent": "^1.5.3",
        "dotenv": "^16.4.5",
        "escalade": "^3.1.2",
        "file-type": "^19.0.0",
        "immer": "^10.1.1",
        "ink": "^5.0.1",
        "ink-big-text": "^2.0.0",
        "ink-gradient": "^3.0.0",
        "ink-spinner": "^5.0.0",
        "neo4j-driver": "^5.21.0",
        "openai": "^4.49.0",
        "package-up": "^5.0.0",
        "pastel": "^3.0.0",
        "react": "^18.3.1",
        "uuid": "^10.0.0",
        "znv": "^0.4.0",
        "zod": "^3.23.8",
        "zustand": "^4.5.2"
    }
}